{
  "name": "linkbox",
  "version": "1.0.0",
  "description": "Link saving app",
  "main": "app.js",
  "scripts": {
    "start": "node ./build/server/app.js",
    "prestart": "npm run build",
    "prebuild": "rm -rf ./build",
    "test": " nyc ---reporter=html --reporter=text mocha --timeout 30000 --exit --require babel-register server/test/* && npm run coverage",
    "build": "babel ./ -d build --ignore node_modules/ --presets=env --source-maps inline --copy-files",
    "dev-start": "babel-watch ./server/app.js",
    "coverage": "nyc report ---reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnayoOleru/LinkBox.git"
  },
  "keywords": [
    "links",
    "box"
  ],
  "author": "Anayo OLeru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnayoOleru/LinkBox/issues"
  },
  "homepage": "https://github.com/AnayoOleru/LinkBox#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "dotenv": "^6.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "mysql": "^2.16.0",
    "nyc": "^13.3.0",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^7.0.0"
  }
}
